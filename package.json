{
  "name": "webpack-typescript-starter",
  "version": "0.0.1",
  "description": "A simple Webpack 4 starter with TypeScript transpilation",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/ --mode development",
    "build": "webpack --config webpack.config.ts --mode production",
    "build.prod": "webpack --config webpack.config.ts -p",
    "build.esnext": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.all": "npm run build.prod && npm run build.esnext",
    "test": "karma start karma.config.js"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "rOman zInchUk",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jasmine": "2.8.7",
    "@types/node": "^14.0.27",
    "@types/webpack": "^4.41.21",
    "@types/webpack-dev-server": "^3.11.0",
    "awesome-typescript-loader": "5.2.0",
    "css-loader": "^4.2.1",
    "html-webpack-plugin": "^4.3.0",
    "jasmine-core": "3.1.0",
    "karma": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "node-sass": "^4.14.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "source-map-loader": "0.2.3",
    "style-loader": "^1.2.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.17.0",
    "tslint-loader": "3.6.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "@types/jquery": "^3.5.1",
    "@types/mathjs": "^6.0.5",
    "jquery": "^3.5.1",
    "mathjs": "^7.1.0"
  }
}
